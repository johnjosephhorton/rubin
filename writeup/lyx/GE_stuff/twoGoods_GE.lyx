\begin_inset Formula 
\begin{align*}
x_{1}^{s}l_{1}=l_{1}x_{1}^{s}C\{1,2\} & \Rightarrow1=C\{1,2\}\\
px_{2}^{s}\left(L^{d}-1\right)=\left(L^{d}-1\right)x_{2}^{s}C\{3,4\} & \Rightarrow p=C\{3,4\}
\end{align*}

\end_inset

